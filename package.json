{
    "name": "miproyecto",
    "version": "1.0.0",
    "engines": {
        "node": ">=8.0.0"
    },
    "description": "",
    "main": "src/app.js",
    "scripts": {
        "start": "node --use_strict ./src/app.js",
        "start-test": "node --use_strict ./src/app.js --test",
        "test": "nyc mocha",
        "e2e": "nyc mocha --config .mocharc.e2e.json",
        "coverage": "nyc mocha -R mocha-junit-reporter --reporter-options mochaFile=./target/junit.xml",
        "wtfmocha": "wtfnode ./node_modules/mocha/bin/_mocha --recursive true --require ./src/**/*.test.js",
        "lint": "nyc eslint src/app.js",
        "autocannon": "node e2e/autocannon/index.js",
        "cucumber": "nyc cucumber-js ./e2e/cucumber/**/*-features -r ./e2e/cucumber/**/step_definitions -r ./e2e/cucumber/support -f json:./target/cucumber/cucumber_report.json -f progress --fail-fast",
        "cucumber-report": "node ./e2e/cucumber/generate-report.js"
    },
    "_moduleAliases": {
        "@root": ".",
        "@config": "src/config",
        "@routes": "src/routes",
        "@common": "src/common",
        "@errors": "src/common/errors",
        "@handlers": "src/common/handlers",
        "@validators": "src/common/validators",
        "@utils": "src/common/utils"
    },
    "nyc": {
        "temp-dir": "./.temp",
        "report-dir": "./target/coverage",
        "reporter": [
            "lcov",
            "cobertura"
        ],
        "exclude": [
            "**/*.test.js",
            "gulpfile.js",
            "target",
            "e2e"
        ]
    },
    "devDependencies": {
        "@types/chai": "4.1.7",
        "@types/crypto-js": "3.1.43",
        "@types/csurf": "1.9.35",
        "@types/cucumber": "4.0.5",
        "@types/express": "4.16.1",
        "@types/faker": "4.1.5",
        "@types/joi": "14.3.3",
        "@types/lodash": "4.14.123",
        "@types/luxon": "1.12.0",
        "@types/mocha": "5.2.6",
        "@types/nock": "10.0.1",
        "@types/node": "11.13.8",
        "@types/passport": "1.0.0",
        "@types/passport-local": "1.0.33",
        "@types/rewire": "2.5.28",
        "@types/sequelize": "4.27.48",
        "@types/sinon": "7.0.11",
        "@types/supertest": "2.0.7",
        "@types/validator": "10.11.0",
        "autocannon": "3.2.1",
        "autocannon-reporter": "0.0.6",
        "chai": "4.2.0",
        "cucumber": "5.1.0",
        "cucumber-html-reporter": "5.0.0",
        "del": "4.1.1",
        "eslint": "5.16.0",
        "eslint-plugin-node": "8.0.1",
        "faker": "4.1.0",
        "gulp": "4.0.1",
        "gulp-install": "1.1.0",
        "gulp-zip": "4.2.0",
        "merge-stream": "1.0.1",
        "mocha": "6.1.4",
        "mocha-junit-reporter": "1.22.0",
        "nyc": "14.0.0",
        "sinon": "7.3.2",
        "supertest": "4.0.2",
        "wtfnode": "0.8.0"
    },
    "dependencies": {
        "bcryptjs": "2.4.3",
        "body-parser": "1.19.0",
        "connect-session-sequelize": "6.0.0",
        "cookie-parser": "1.4.4",
        "crypto-js": "3.1.9-1",
        "csurf": "1.10.0",
        "express": "4.16.4",
        "express-session": "1.16.1",
        "helmet": "3.16.0",
        "hpp": "0.2.2",
        "joi": "14.3.1",
        "lodash": "4.17.11",
        "luxon": "1.12.1",
        "mariadb": "2.0.3",
        "module-alias": "2.2.0",
        "moment": "2.24.0",
        "morgan": "1.9.1",
        "passport": "0.4.0",
        "passport-local": "1.0.0",
        "rotating-file-stream": "1.4.1",
        "sequelize": "5.8.1",
        "validator": "10.11.0",
        "winston": "3.2.1",
        "winston-daily-rotate-file": "3.9.0"
    }
}
